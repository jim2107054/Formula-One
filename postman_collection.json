{
  "info": {
    "name": "Formula One - Complete API Testing",
    "description": "Complete API collection for testing Backend (Node.js) and AI-Backend (Python) services",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "Backend - Authentication",
      "item": [
        {
          "name": "Register User",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"testuser@example.com\",\n  \"password\": \"password123\",\n  \"name\": \"Test User\",\n  \"role\": 0\n}"
            },
            "url": {
              "raw": "http://localhost:4000/api/auth/register",
              "protocol": "http",
              "host": ["localhost"],
              "port": "4000",
              "path": ["api", "auth", "register"]
            },
            "description": "Register a new user (role: 0=student, 1=instructor, 2=admin)"
          }
        },
        {
          "name": "Login",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"admin@formulaone.com\",\n  \"password\": \"admin123\"\n}"
            },
            "url": {
              "raw": "http://localhost:4000/api/auth/login",
              "protocol": "http",
              "host": ["localhost"],
              "port": "4000",
              "path": ["api", "auth", "login"]
            },
            "description": "Login with email and password"
          }
        },
        {
          "name": "Get Current User",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}",
                "description": "JWT token from login response"
              }
            ],
            "url": {
              "raw": "http://localhost:4000/api/auth/me",
              "protocol": "http",
              "host": ["localhost"],
              "port": "4000",
              "path": ["api", "auth", "me"]
            },
            "description": "Get current user profile"
          }
        },
        {
          "name": "Logout",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}"
              }
            ],
            "url": {
              "raw": "http://localhost:4000/api/auth/logout",
              "protocol": "http",
              "host": ["localhost"],
              "port": "4000",
              "path": ["api", "auth", "logout"]
            },
            "description": "Logout current user"
          }
        }
      ]
    },
    {
      "name": "Backend - Chat",
      "item": [
        {
          "name": "Create Chat Session",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "Authorization",
                "value": "Bearer {{token}}"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"title\": \"Physics Discussion\"\n}"
            },
            "url": {
              "raw": "http://localhost:4000/api/chat/session",
              "protocol": "http",
              "host": ["localhost"],
              "port": "4000",
              "path": ["api", "chat", "session"]
            },
            "description": "Create a new chat session"
          }
        },
        {
          "name": "Send Message",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "Authorization",
                "value": "Bearer {{token}}"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"sessionId\": \"{{sessionId}}\",\n  \"content\": \"Explain Newton's First Law of Motion\"\n}"
            },
            "url": {
              "raw": "http://localhost:4000/api/chat/message",
              "protocol": "http",
              "host": ["localhost"],
              "port": "4000",
              "path": ["api", "chat", "message"]
            },
            "description": "Send a message and get AI response"
          }
        },
        {
          "name": "Get Chat History",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}"
              }
            ],
            "url": {
              "raw": "http://localhost:4000/api/chat/history",
              "protocol": "http",
              "host": ["localhost"],
              "port": "4000",
              "path": ["api", "chat", "history"]
            },
            "description": "Get all chat sessions for current user"
          }
        },
        {
          "name": "Get Specific Session",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}"
              }
            ],
            "url": {
              "raw": "http://localhost:4000/api/chat/session/{{sessionId}}",
              "protocol": "http",
              "host": ["localhost"],
              "port": "4000",
              "path": ["api", "chat", "session", "{{sessionId}}"]
            },
            "description": "Get a specific chat session by ID"
          }
        },
        {
          "name": "Delete Session",
          "request": {
            "method": "DELETE",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}"
              }
            ],
            "url": {
              "raw": "http://localhost:4000/api/chat/session/{{sessionId}}",
              "protocol": "http",
              "host": ["localhost"],
              "port": "4000",
              "path": ["api", "chat", "session", "{{sessionId}}"]
            },
            "description": "Delete a chat session"
          }
        }
      ]
    },
    {
      "name": "Backend - Health",
      "item": [
        {
          "name": "Health Check",
          "request": {
            "method": "GET",
            "url": {
              "raw": "http://localhost:4000/api/health",
              "protocol": "http",
              "host": ["localhost"],
              "port": "4000",
              "path": ["api", "health"]
            },
            "description": "Check backend service health"
          }
        }
