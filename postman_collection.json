        },
        {
          "name": "Service Info",
          "request": {
            "method": "GET",
            "url": {
              "raw": "http://localhost:8000/",
              "protocol": "http",
              "host": ["localhost"],
              "port": "8000",
              "path": []
            },
            "description": "Get AI backend service information"
          }
        }
      ]
    },
    {
      "name": "AI-Backend - Generation",
      "item": [
        {
          "name": "Generate Explanation",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"prompt\": \"Explain the concept of photosynthesis in plants\",\n  \"generation_type\": \"explanation\",\n  \"context\": []\n}"
            },
            "url": {
              "raw": "http://localhost:8000/api/v1/generation/generate",
              "protocol": "http",
              "host": ["localhost"],
              "port": "8000",
              "path": ["api", "v1", "generation", "generate"]
            },
            "description": "Generate explanation for a given prompt"
          }
        },
        {
          "name": "Generate Code",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"prompt\": \"Write a Python function to calculate factorial\",\n  \"generation_type\": \"code\",\n  \"context\": []\n}"
            },
            "url": {
              "raw": "http://localhost:8000/api/v1/generation/generate",
              "protocol": "http",
              "host": ["localhost"],
              "port": "8000",
              "path": ["api", "v1", "generation", "generate"]
            },
            "description": "Generate code snippet for a given prompt"
          }
        }
      ]
    },
    {
      "name": "AI-Backend - Validation",
      "item": [
        {
          "name": "Validate Code",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"code\": \"def factorial(n):\\n    if n <= 1:\\n        return 1\\n    return n * factorial(n-1)\",\n  \"language\": \"python\"\n}"
            },
            "url": {
              "raw": "http://localhost:8000/api/v1/validation/validate",
              "protocol": "http",
              "host": ["localhost"],
              "port": "8000",
              "path": ["api", "v1", "validation", "validate"]
            },
            "description": "Validate code syntax and quality"
          }
        }
      ]
    },
    {
      "name": "AI-Backend - RAG (Retrieval Augmented Generation)",
      "item": [
        {
          "name": "Search Documents",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"query\": \"What is machine learning?\",\n  \"top_k\": 5\n}"
            },
            "url": {
              "raw": "http://localhost:8000/api/v1/search",
              "protocol": "http",
              "host": ["localhost"],
              "port": "8000",
              "path": ["api", "v1", "search"]
            },
            "description": "Search through documents using RAG"
          }
        }
      ]
    }
  ],
  "variable": [
    {
      "key": "token",
      "value": "",
      "type": "string",
      "description": "JWT token from login - copy from login response and paste here"
    },
    {
      "key": "sessionId",
